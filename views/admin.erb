<p id="buttonExport" style="text-align: right">
  <a href="/admin/download" class="button info medium">Export to CSV</a>
</p>
<ul style="display: inline-block; ">
  <li><a class="button medium" href="/admin">last week</a></li>
  <li><a class="button medium" href="/admin/month">last month</a></li>
</ul>
<table class="table is-hoverable is-fullwidth">
  <thead>
    <tr>
      <th><abbr title="number">#</abbr></th>
      <th>User</th>
      <th>Registered Days</th>
    </tr>
  </thead>
  <tbody>
    <% @data.each_with_index do |user, index| %>
    <tr>
      <td><%= index+1 %></td>
      <td><%= user[:name] %></td>
      <td><%= user[:days] %></td>
    </tr>
    <% end %>
  </tbody>
</table>
